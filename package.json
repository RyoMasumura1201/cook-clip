{
  "name": "mynextapp",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "eslint src --ext .js,jsx,.ts,.tsx --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "prismaDev": "dotenv -e .env.local prisma migrate dev ",
    "prismaStudio": "dotenv -e .env.local prisma studio ",
    "prismaPush": "dotenv -e .env.local prisma db push "
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@hookform/resolvers": "^2.8.8",
    "@prisma/client": "^3.14.0",
    "axios": "^0.26.1",
    "dayjs": "^1.11.2",
    "framer-motion": "^5",
    "nanoid": "^3.3.1",
    "next": "12.0.9",
    "next-auth": "^4.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.28.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.16",
    "react-youtube": "^7.14.0",
    "recoil": "^0.6.1",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/node": "17.0.12",
    "@types/react": "17.0.38",
    "@types/recoil": "^0.0.9",
    "@types/youtube": "^0.0.46",
    "dotenv-cli": "^5.0.0",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "prisma": "^3.14.0",
    "typescript": "4.5.5"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100
  }
}
